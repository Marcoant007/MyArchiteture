  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect=0.0.0.0:5858 --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "dev:server": "ts-node-dev --unhandled-rejections=strict --inspect=0.0.0.0:5858 --transpile-only --ignore-watch node_modules src/server.ts",
    "test": "jest --coverage --watchAll --colors",
    "test:hmg": "jest --no-cache --coverage --ci",
    "test:sonar": "node sonar.js",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "watch": "concurrently \"npm run dev:server\" \"npm run test\" "
  },
